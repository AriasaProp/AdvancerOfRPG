{
  BitmapFont: {
    board_font: {
      file: game-continue_50px.fnt
    }
    title_font: {
      file: immortal_60px.fnt
    }
    small_font: {
      file: sans-serif_30px.fnt
    }
    default_font: {
      file: arial_32px.fnt
    }
    med: {
      file: goudy-med_40px.fnt
    }
  }
  Color: {
    gray: {
      hex: 444444ff
    }
    highlight: {
      hex: 00ecffff
    }
    panel: {
      hex: 000000af
    }
    acccount_color: {
      hex: 00a1a0ff
    }
    black: {
      hex: 000000ff
    }
    darken_color: {
      hex: 515151ff
    }
    enable_button: {
      hex: 25dff2ff
    }
    event_color: {
      hex: 00c090ff
    }
    light_gray: {
      hex: d1d1d1ff
    }
    menu: {
      hex: 00a5c1ff
    }
    pressed: {
      hex: 8d8d8dff
    }
    select_color: {
      hex: 1172abff
    }
    transprnt: {
      hex: 7d7d7d7e
    }
    dark_menu: {
      hex: 00007fff
    }
    white: {
      hex: ffffffff
    }
  }
  TintedDrawable: {
    proggres_hor_fill: {
      name: white
      color: highlight
      minWidth: 50
      minHeight: 50
    }
    proggres_hor_back: {
      name: white
      color: {
        hex: 909090ff
      }
      minWidth: 50
      minHeight: 50
    }
    panel_back: {
      name: white
      color: panel
    }
    button_main_up: {
      name: button_glassy_up
      color: menu
    }
    round_large_dark: {
      name: round_large
      color: darken_color
    }
    button_main_down: {
      name: button_glassy_down
      color: menu
    }
    select_tint: {
      name: white
      color: select_color
    }
    event_background: {
      name: round_large
      color: event_color
    }
    account_background: {
      name: round_large
      color: acccount_color
    }
    config_tab_up: {
      name: round
      color: enable_button
    }
    round_dark: {
      name: round
      color: darken_color
    }
    round_border_dark: {
      name: round_border
      color: darken_color
    }
    panel: {
      name: round_large
      color: transprnt
    }
    noneDraw: {
      name: act_up
      color: white
    }
    split_line: {
      name: white
      color: highlight
      minWidth: 5
      minHeight: 5
    }
  }
  ButtonStyle: {
    default: {
      up: button_glassy_up
      down: button_glassy_down
    }
  }
  CheckBoxStyle: {
    default: {
      checkboxOn: check_on
      checkboxOff: check_off
      font: small_font
    }
  }
  ImageButtonStyle: {
    default: {
      imageUp: noneDraw
    }
    config: {
      imageUp: config_icon
      up: round_border
      down: round_border_dark
    }
    close: {
      imageUp: close_icon
      up: round_border
      down: round_border_dark
    }
    attack: {
      imageUp: attack_up
      imageDown: attack_down
    }
    jump: {
      imageUp: jump_up
      imageDown: jump_down
    }
    act: {
      imageUp: act_up
      imageDown: act_down
    }
  }
  ImageTextButtonStyle: {
    default: {
      parent: default
      font: small_font
    }
  }
  LabelStyle: {
    default: {
      font: small_font
      background: panel_back
    }
    title: {
      font: title_font
    }
    med: {
      font: med
    }
  }
  ListStyle: {
    default: {
      font: small_font
      fontColorSelected: light_gray
      fontColorUnselected: transprnt
      selection: select_tint
      background: darken
    }
  }
  ProgressBarStyle: {
    default-horizontal: {
      knobAfter: proggres_hor_back
      knobBefore: proggres_hor_fill
    }
    default-vertical: {
      knobAfter: proggres_hor_back
      knobBefore: proggres_hor_fill
    }
  }
  ScrollPaneStyle: {
    default: {
      background: round_large_dark
    }
  }
  SelectBoxStyle: {
    default: {
      font: small_font
      fontColor: white
      background: button_expand
      scrollStyle: default
      listStyle: default
      backgroundOpen: round_large
    }
  }
  SliderStyle: {
    default-horizontal: {
      background: round_dark
      knob: round
    }
    default-vertical: {
      background: round_dark
      knob: round
    }
  }
  SplitPaneStyle: {
    default-horizontal: {
      handle: split_line
    }
    default-vertical: {
      parent: default-horizontal
      handle: split_line
    }
  }
  TextButtonStyle: {
    default: {
      font: small_font
      up: button_metalic_up
      down: button_metalic_down
    }
    menu: {
      font: board_font
      up: button_main_up
      down: button_main_down
    }
    tab: {
      font: board_font
      downFontColor: black
      up: label_grand
      down: strgrd_pan
    }
    config_tab: {
      font: small_font
      checkedFontColor: pressed
      up: config_tab_up
      checked: round_dark
    }
  }
  TextFieldStyle: {
    default: {
      font: small_font
      fontColor: white
      background: round_large
    }
  }
  TouchpadStyle: {
    default: {
      background: touchpad_background
      knob: touchpad_knob
    }
  }
  TreeStyle: {
    default: {
      plus: tree
      minus: tree-min
    }
  }
  WindowStyle: {
    default: {
      background: round_large_dark
      titleFont: board_font
    }
  }
}
